<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".order.ShipmentTrackingFragment">

    <!-- TODO: Update blank fragment layout -->
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/textViewShipmentID"
            android:layout_width="290dp"
            android:layout_height="39dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="56dp"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewShipmentStatus"
            android:layout_width="288dp"
            android:layout_height="39dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="40dp"
            android:textSize="18sp"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewShipmentID" />

        <TextView
            android:id="@+id/textViewShipmentNote"
            android:layout_width="336dp"
            android:layout_height="275dp"
            android:layout_marginStart="36dp"
            android:layout_marginTop="32dp"
            android:textSize="16sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textViewShipmentStatus" />
    </android.support.constraint.ConstraintLayout>

</FrameLayout>